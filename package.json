{
	"author": "Paul Williams",
	"name": "p90",
	"description": "A minimalist CSS pre-processor for Svelte.",
	"keywords": [
		"svelte",
		"sveltekit",
		"css",
		"preprocessor",
		"minimalist"
	],
	"version": "0.14.0",
	"exports": {
		".": "./src/lib/index.js",
		"./util": "./src/lib/util.js"
	},
	"type": "module",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0 <19"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/p90.git"
	},
	"scripts": {
		"fmt": "npx prettier --loglevel warn --write --plugin-search-dir=. .",
		"build": "npm run fmt && vite build",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --silent",
		"debug": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"link": "npm link && npm link p90",
		"dev": "npm run fmt && vite dev --port 3000",
		"commit": "npm run test && npm run build && cat ./scripts/youre-ready-to-integrate.txt"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.5",
		"jest": "29.5.0",
		"p90": "0.14.0",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "4.0.0",
		"vite": "4.3.9"
	}
}
